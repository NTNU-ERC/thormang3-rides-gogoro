<?xml version="1.0"?>
<launch>

    <arg name="paused" default="true"/>
    <arg name="use_sim_time" default="true"/>   
    <arg name="gui" default="true"/>
    <arg name="headless" default="false"/>
    <arg name="debug" default="false"/>

    <!-- include gazebo_ros launcher-->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find gogoro_description)/worlds/empty.world"/>
        <arg name="debug" value="$(arg debug)"/>
        <arg name="gui" value="$(arg gui)"/>
        <arg name="paused" value="$(arg paused)"/>
        <arg name="use_sim_time" value="$(arg use_sim_time)"/>
        <arg name="headless" value="$(arg headless)"/>
    </include>
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find gogoro_description)/urdf/gogoro_and_thormang3.xacro'"/>

    <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model gogoro_and_thormang3 
        -z 2.0
        -J l_arm_el_y -0.844
        -J l_arm_wr_r -0.260
        -J head_y -0.003
        -J r_arm_grip 1.060
        -J l_arm_wr_p -0.076
        -J torso_y -0.052
        -J r_arm_sh_r -1.613
        -J l_arm_sh_p1 0.214
        -J l_arm_sh_r 1.459
        -J l_leg_an_r 0.120
        -J l_leg_an_p -0.390
        -J r_leg_hip_p 1.165
        -J r_leg_an_p 0.354
        -J l_arm_wr_y -0.242
        -J l_leg_hip_p -1.193
        -J r_leg_hip_y 0.277
        -J l_leg_hip_r 0.110
        -J l_leg_kn_p 0.958
        -J r_arm_sh_p2 0.288
        -J r_arm_sh_p1 -0.221
        -J l_leg_hip_y -0.095
        -J r_leg_hip_r -0.194
        -J l_arm_sh_p2 0.036
        -J r_arm_wr_y 0.160
        -J head_p 0.002
        -J r_arm_wr_p -0.027
        -J r_arm_wr_r 0.175
        -J r_arm_el_y 0.876
        -J l_arm_grip 1.043
        -J r_leg_an_r -0.162
        -J r_leg_kn_p -0.975
        "/>
    
    <include file="$(find gogoro_description)/launch/position_controller.launch"/>
    <include file="$(find thormang3_gazebo)/launch/position_controller.launch"/>
    
</launch>
